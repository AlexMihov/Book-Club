{
  "name": "Bookclub",
  "version": "1.0.0",
  "description": "An app to organize any Bookclub",
  "main": "index.js",
  "scripts": {
    "start": "node ./node_modules/better-npm-run start",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "watch-client": "node ./node_modules/better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\""
  },
  "betterScripts": {
    "start": {
      "command": "node ./index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "DEBUG": "bookclub:*"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "DEBUG": "bookclub:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://AlexMihov@github.com/AlexMihov/Book-Club.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexMihov/Book-Club/issues"
  },
  "homepage": "https://github.com/AlexMihov/Book-Club",
  "dependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel": "^5.8.20",
    "babel-loader": "^5.3.2",
    "babel-plugin-typecheck": "^1.2.0",
    "babel-runtime": "^5.8.20",
    "better-npm-run": "0.0.1",
    "clean-webpack-plugin": "^0.1.3",
    "css-loader": "^0.15.6",
    "debug": "^2.2.0",
    "express": "^4.13.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "jade": "^1.11.0",
    "lodash": "^3.10.0",
    "node-sass": "^3.2.0",
    "piping": "^0.2.0",
    "react": "^0.13.3",
    "react-hot-loader": "^1.2.8",
    "react-redux": "^0.2.2",
    "redux": "^1.0.0-rc",
    "redux-actions": "^0.7.0",
    "redux-devtools": "^0.1.2",
    "rethinkdb": "^2.0.2",
    "sass-loader": "^1.0.3",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.12.3",
    "thinky": "^2.0.10",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1"
  }
}
